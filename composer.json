{
    "name": "fedapay/fedapay-php",
    "description": "fedapay PHP Library",
    "keywords": [
        "fedapay",
        "payment processing",
        "api"
    ],
    "homepage": "https://fedapay.com/",
    "license": "MIT",
    "authors": [{
        "name": "Fedapay and contributors",
        "homepage": "https://gitlab.com/nautilus-technology/fedapay-php/contributors"
    }],
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "satooshi/php-coveralls": "~0.6.1",
        "squizlabs/php_codesniffer": "~2.0",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Fedapay\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
